{
    "executionRoleArn": "arn:aws:iam::826109606486:role/ecsTaskExecutionRole",
    "family": "demo-angular-app",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "demo-angular-app",
            "image": "826109606486.dkr.ecr.us-east-1.amazonaws.com/demo-angular-app:0.2.0",
            "cpu": 256,
            "memory": 512,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "cpu": "256", 
    "memory": "512",
    "requiresCompatibilities": [
        "FARGATE"
    ]
}